<p id="notice"><%= notice %></p>

<h1>Data Cleanings</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_cleanings.each do |data_cleaning| %>
      <tr>
        <td><%= link_to 'Show', data_cleaning %></td>
        <td><%= link_to 'Edit', edit_data_cleaning_path(data_cleaning) %></td>
        <td><%= link_to 'Destroy', data_cleaning, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data Cleaning', new_data_cleaning_path %>
