<h1>Project Datum Columns</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_datum_columns.each do |project_datum_column| %>
      <tr>
        <td><%= link_to 'Show', project_datum_column %></td>
        <td><%= link_to 'Edit', edit_project_datum_column_path(project_datum_column) %></td>
