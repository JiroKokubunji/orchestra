<p id="notice"><%= notice %></p>
<div class="row">
  <div class="col col-lg-3">
    <h1>Project List</h1>
  </div>
</div>

<div class="row">
  <div class="col col-lg-12">
    <table class="table table-hover">
      <thead>
        <tr>
          <th>Name</th>
          <th>Created At</th>
        </tr>
      </thead>
      <tbody>
        <% @projects.each do |project| %>
        <tr class="js-project-row" data-project-id="<%= project.id %>">
          <td><%= project.name %></td>
          <td><%= project.created_at %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
